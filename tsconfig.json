{
	"compilerOptions": {
		"target": "ES2019",
		"lib": ["DOM", "DOM.Iterable", "ES2017", "ES2021.String"],
		
		"rootDir": "./src",
		"outDir": "./out",
		"removeComments": true,
		"noEmitOnError": true,
		"isolatedModules": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,

		"strict": true,
		"noImplicitAny": true,
		"noFallthroughCasesInSwitch": false,
		// "noPropertyAccessFromIndexSignature": true,
		"skipLibCheck": true
	}
}
